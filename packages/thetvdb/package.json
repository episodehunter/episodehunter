{
  "name": "@episodehunter/thetvdb",
  "description": "Helper lib for thetvdb api",
  "version": "2.0.1",
  "scripts": {
    "test": "jest",
    "compile:watch": "tsc --watch",
    "prepublishOnly": "tsc"
  },
  "author": "tjoskar <kontakta@oskarkarlsson.nu>",
  "bugs": {
    "url": "https://github.com/episodehunter/thetvdb/issues",
    "email": "kontakta@oskarkarlsson.nu"
  },
  "dependencies": {
    "@types/node-fetch": "^1.6.7",
    "node-fetch": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "22.1.3",
    "jest": "22.4.2",
    "simple-spy": "^2.2.1",
    "ts-jest": "22.0.4",
    "typescript": "2.7.2"
  },
  "homepage": "https://github.com/episodehunter/thetvdb",
  "keywords": [
    ""
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/episodehunter/thetvdb"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/types/"
    ]
  }
}
